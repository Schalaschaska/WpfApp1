<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="413" Width="592">
    <Window.Resources>
        <Image x:Key="Object1" Source="/images/editor.png"/>
        <Image x:Key="Object2" Source="/images/tools.png"/>
        <Image x:Key="Object3" Source="/images/home.png"/>
    </Window.Resources>
    <Grid>
        <Frame Content="" HorizontalAlignment="Left" Height="297" VerticalAlignment="Top" Width="584" Name="navigator" Margin="0,85,0,0" />
        <Button Content="{DynamicResource Object1}" HorizontalAlignment="Left" Margin="114,10,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" ToolTip="Новый расчёт"/>
        <Button Content="{DynamicResource Object2}" HorizontalAlignment="Left" Margin="213,10,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1" ToolTip="Настройки"/>
        <Button Content="{DynamicResource Object3}" HorizontalAlignment="Left" Margin="19,10,0,0" VerticalAlignment="Top" Width="75" Height="75" Click="Button_Click_2" ToolTip="Домашняя страница"/>

    </Grid>
</Window>
